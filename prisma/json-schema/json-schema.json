{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "Person": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "phone": {
          "type": "string"
        },
        "phoneVerified": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "name": {
          "type": [
            "string",
            "null"
          ]
        },
        "image": {
          "type": [
            "string",
            "null"
          ]
        },
        "role": {
          "type": "string",
          "default": "CONTACT",
          "enum": [
            "CONTACT",
            "CLIENT",
            "PATIENT",
            "DOCTOR",
            "MEMBER",
            "USER"
          ]
        },
        "createdAt": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "deletedAt": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "updatedAt": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "issues": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Issue"
          }
        },
        "user": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/User"
          }
        }
      }
    },
    "User": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "person": {
          "$ref": "#/definitions/Person"
        },
        "membership": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Member"
          }
        },
        "invitedMembers": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Member"
          }
        },
        "issues": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Issue"
          }
        },
        "createdOrganizations": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Organization"
          }
        }
      }
    },
    "Organization": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "domain": {
          "type": "string"
        },
        "inviteCode": {
          "type": "string"
        },
        "name": {
          "type": [
            "string",
            "null"
          ]
        },
        "createdAt": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "deletedAt": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "updatedAt": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "creator": {
          "$ref": "#/definitions/User"
        },
        "members": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Member"
          }
        }
      }
    },
    "Member": {
      "type": "object",
      "properties": {
        "invitedAt": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "joinedAt": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "deletedAt": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "updatedAt": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "status": {
          "type": "string",
          "default": "PENDING",
          "enum": [
            "ACCEPTED",
            "PENDING",
            "DECLINED"
          ]
        },
        "teamRole": {
          "type": "string",
          "default": "MEMBER",
          "enum": [
            "MEMBER",
            "OWNER"
          ]
        },
        "member": {
          "$ref": "#/definitions/User"
        },
        "organization": {
          "$ref": "#/definitions/Organization"
        },
        "invitedBy": {
          "anyOf": [
            {
              "$ref": "#/definitions/User"
            },
            {
              "type": "null"
            }
          ]
        }
      }
    },
    "Issue": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "assignee": {
          "anyOf": [
            {
              "$ref": "#/definitions/User"
            },
            {
              "type": "null"
            }
          ]
        },
        "reporter": {
          "$ref": "#/definitions/Person"
        }
      }
    }
  },
  "type": "object",
  "properties": {
    "person": {
      "$ref": "#/definitions/Person"
    },
    "user": {
      "$ref": "#/definitions/User"
    },
    "organization": {
      "$ref": "#/definitions/Organization"
    },
    "member": {
      "$ref": "#/definitions/Member"
    },
    "issue": {
      "$ref": "#/definitions/Issue"
    }
  }
}